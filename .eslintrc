{
    "env": {
        "es6": true,
        "node": true,
        "jasmine": true,
        "jest": true,
        "mocha": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parser": "babel-eslint",
    "rules": {
        "no-underscore-dangle": 0,
        "import/no-cycle": 0,
        "max-len": ["warn", 140],
        "no-param-reassign": ["error", { "props": false }]
    }
}
